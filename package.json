{
  "name": "@deljuse/tree-core",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && move-cli lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deljus/tree-core.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "main": "./lib/cjs/index.js",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [],
  "author": "deljuse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deljus/tree-core/issues"
  },
  "homepage": "https://github.com/deljus/tree-core#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@types/uuid": "^9.0.0",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "move-cli": "^2.0.0",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  }
}
